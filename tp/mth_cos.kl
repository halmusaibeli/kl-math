PROGRAM mth_cos
%NOBUSYLAMP
%NOLOCKGROUP

VAR
	out_reg : INTEGER
	angle : REAL
%from tpe.klh %import get_int_arg,get_real_arg
%from registers.klh %import set_real
%from math.builtins.klh %import cos

BEGIN
	angle = tpe__get_real_arg(1)
	out_reg = tpe__get_int_arg(2)
	registers__set_real(out_reg, math__cos(angle))
END mth_cos