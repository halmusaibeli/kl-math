PROGRAM mth_arcang
%NOBUSYLAMP
%NOLOCKGROUP

VAR
	out_reg : INTEGER
	length : REAL
	radius : REAL
%from tpe.klh %import get_int_arg,get_real_arg
%from registers.klh %import set_real
%from math.klh %import arcangle

BEGIN
	length = tpe__get_real_arg(1)
	radius = tpe__get_real_arg(2)
	out_reg = tpe__get_int_arg(3)
	registers__set_real(out_reg, math__arcangle(length,radius))
END mth_arcang